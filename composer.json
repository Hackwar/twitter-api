{
    "name": "joomla/twitter",
    "type": "joomla-package",
    "description": "Joomla Twitter Package",
    "keywords": ["joomla", "framework", "twitter"],
    "homepage": "https://github.com/joomla-framework/twitter-api",
    "license": "GPL-2.0+",
    "require": {
        "php": ">=5.3.10",
        "joomla/oauth1": "2.0.*@dev",
        "joomla/input": "~1.0@stable",
        "joomla/http": "~1.0@stable",
        "joomla/uri": "~1.0@stable"
    },
    "require-dev": {
        "joomla/test": "~1.0@stable",
        "phpunit/phpunit": "4.*@stable",
        "squizlabs/php_codesniffer": "1.*@stable"
    },
    "suggest": {
        "joomla/registry": "Registry can be used as an alternative to using an array for the package options."
    },
    "autoload": {
        "psr-4": {
            "Joomla\\Twitter\\": "src/",
            "Joomla\\Twitter\\Tests\\": "Tests/"
        }
    },
    "extra": {
        "branch-alias": {
	        "dev-2.0-dev": "2.0-dev"
        }
    },
    "minimum-stability": "dev"
}

